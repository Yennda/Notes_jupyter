    "def process(table):\n",
    "    data = []\n",
    "    rows = table.split('\\n')\n",
    "\n",
    "    for row in rows:\n",
    "        data.append([float(i) for i in row.split(' ')])\n",
    "    return np.array(data)\n",
    "    "
   ]
